record(calcout, "gateway:auto:cnt") {
  field(SCAN, "1 second")
  field(CALC, "A+B")
  field(INPA, "gateway:auto:cnt.VAL NPP")
  field(INPB, "0.01")
  field(OCAL, "sin(A)")
  field(DOPT, "Use OCAL")
  field(OUT , "gateway:auto PP")
}

record(ai, "gateway:auto") {
  field(PREC , 10)
  field(DESC , "A PV that changes once a second")
}

